{
  "assets": {
    "uris": {
      "jre-tar-gz": "{{jre-url}}",
      "libmesos-bundle-tar-gz": "{{libmesos-bundle-url}}",
      "bootstrap-tar-gz": "{{artifact-dir}}/bootstrap.tar.gz",
      "scheduler-zip": "{{artifact-dir}}/nifi-scheduler.zip",
      "executor-zip": "{{artifact-dir}}/executor.zip",
      "nifi-tar-gz" : "https://s3.amazonaws.com/vishnu-mohan/nifi/nifi-1.3.0-bin.tar.gz"
    }
  },
  "images": {
    "icon-small": "https://s3.amazonaws.com/vishnu-mohan/nifi/nifi_small.png",
    "icon-medium": "https://s3.amazonaws.com/vishnu-mohan/nifi/nifi_medium.png",
    "icon-large": "https://s3.amazonaws.com/vishnu-mohan/nifi/nifi_large.png"
  },
  "cli":{
    "binaries":{
      "darwin":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-nifi-darwin}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-nifi-darwin"
        }
      },
      "linux":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-nifi-linux}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-nifi-linux"
        }
      },
      "windows":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-nifi.exe}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-nifi.exe"
        }
      }
    }
  }
}
